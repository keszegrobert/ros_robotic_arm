<robot name="test_robot">
  <link name="base_link">
    <visual>
      <geometry>
          <mesh filename="package://owi535/meshes/accupack.dae"/>
      </geometry>
    </visual>
  </link>

  <link name="base" >
    <visual>
      <geometry>
          <mesh filename="package://owi535/meshes/shoulder.dae"/>
      </geometry>
    </visual>
  </link>

  <link name="humerus" >
    <visual>
      <geometry>
          <mesh filename="package://owi535/meshes/humerus.dae"/>
      </geometry>
    </visual>
  </link>

  <link name="radius" >
    <visual>
      <geometry>
          <mesh filename="package://owi535/meshes/radius.dae"/>
      </geometry>
    </visual>
  </link>

  <link name="gripper" >
    <visual>
      <geometry>
          <mesh filename="package://owi535/meshes/gripper.dae"/>
      </geometry>
    </visual>
